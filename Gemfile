source "https://rubygems.org"

# Use GitHub Pages' pinned Jekyll + plugin versions
gem "github-pages", group: :jekyll_plugins
gem "jekyll-theme-minimal"

# Needed for `jekyll serve` on Ruby 3+
gem "webrick", "~> 1.8"

group :jekyll_plugins do
  gem "jekyll-feed"
  gem "jekyll-seo-tag"
  gem "jekyll-sitemap"
end
